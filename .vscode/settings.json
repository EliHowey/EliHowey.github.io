{
	"liveSassCompile.settings.formats": [
		{
			"format": "compressed",
			"extensionName": ".min.css",
			"savePath": "/dist/styles"
		}
	],
	"liveSassCompile.settings.autoprefix": [
		"> 1%",
		"last 2 versions"
	],
	"search.exclude": {
		"**/*.js: {\"when\": \"$(basename).ts\"}": true,
		"**/*.js.map: {\"when\": \"$(basename).ts\"}": true
	}
}